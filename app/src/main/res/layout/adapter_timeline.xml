    <!-- /////////////1 Start1 ///////////////layout for SHARE/////////////////// -->

    <LinearLayout
        android:id="@+id/lnr_share"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/feed_item_margin"
        android:background="@drawable/bg_parent_rounded_corner"
        android:orientation="vertical"
        android:paddingBottom="@dimen/feed_item_padding_top_bottom"
        android:paddingTop="@dimen/feed_item_padding_top_bottom"
        android:visibility="gone" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingLeft="@dimen/feed_item_padding_left_right"
            android:paddingRight="@dimen/feed_item_padding_left_right" >

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <ImageView
                    android:id="@+id/profilePic"
                    android:layout_width="@dimen/feed_item_profile_pic"
                    android:layout_height="@dimen/feed_item_profile_pic"
                    android:scaleType="fitCenter" />

                <TextView
                    android:id="@+id/tv_profilePic"
                    android:layout_width="@dimen/feed_item_profile_pic"
                    android:layout_height="@dimen/feed_item_profile_pic"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/circle_prop_bg"
                    android:gravity="center"
                    android:textColor="#FFFFFFFF"
                    android:textSize="45sp"
                    android:visibility="gone" />
            </RelativeLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingLeft="@dimen/feed_item_profile_info_padd" >

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:background="@drawable/grid_row_selector"
                        android:text="Ravi Gupta"
                        android:textSize="@dimen/feed_item_profile_name"
                        android:textStyle="bold" />

                    <ImageView
                        android:id="@+id/menu_delete"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:padding="2dp"
                        android:src="@mipmap/three_dots_black" />
                </RelativeLayout>

                <TextView
                    android:id="@+id/txtContent"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="has shared his status" />

                <TextView
                    android:id="@+id/timestamp"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="07:40 PM"
                    android:textColor="@color/timestamp"
                    android:textSize="@dimen/feed_item_timestamp" />
            </LinearLayout>
        </LinearLayout>

        <TextView
            android:id="@+id/txtShareContent"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:linksClickable="true"
            android:paddingBottom="10dp"
            android:paddingLeft="@dimen/feed_item_status_pad_left_right"
            android:paddingRight="@dimen/feed_item_status_pad_left_right"
            android:text="status message"
            android:textColorLink="@color/link" />

        <TextView
            android:id="@+id/txtStatusMsg1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:linksClickable="true"
            android:paddingBottom="10dp"
            android:paddingLeft="@dimen/feed_item_status_pad_left_right"
            android:paddingRight="@dimen/feed_item_status_pad_left_right"
            android:text="status message"
            android:textColorLink="@color/link" />

        <LinearLayout
            android:id="@+id/lnr_shareContent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <ImageView
                    android:id="@+id/iv_sharedContent"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="5dp"
                    android:paddingLeft="5dp"
                    android:scaleType="fitCenter" />

                <TextView
                    android:id="@+id/tv_sharedContent"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="5dp"
                    android:paddingLeft="5dp"
                    android:background="@drawable/circle_prop_bg"
                    android:gravity="center"
                    android:textColor="#FFFFFFFF"
                    android:textSize="30sp"
                    android:visibility="gone" />
            </RelativeLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingLeft="@dimen/feed_item_profile_info_padd" >

                <TextView
                    android:id="@+id/tv_nameShare"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/grid_row_selector"
                    android:linksClickable="true"
                    android:paddingBottom="@dimen/feed_item_margin"
                    android:paddingLeft="@dimen/feed_item_margin"
                    android:paddingRight="@dimen/feed_item_margin"
                    android:text="Ravi Gupt"
                    android:textColorLink="@color/link"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/txtStatusMsg11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:linksClickable="true"
                    android:paddingLeft="@dimen/feed_item_margin"
                    android:paddingRight="@dimen/feed_item_margin"
                    android:text="status message"
                    android:textColorLink="@color/link" />
            </LinearLayout>
        </LinearLayout>

        <ImageView
            android:id="@+id/feedImage1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:scaleType="fitXY"
            android:visibility="gone" />

        <View
            android:layout_width="match_parent"
            android:layout_height=".5dp"
            android:background="#11000000" />

        <TextView
            android:id="@+id/txtTotalLikes"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="2dp"
            android:paddingLeft="@dimen/feed_item_status_pad_left_right"
            android:paddingRight="@dimen/feed_item_status_pad_left_right"
            android:text="00 Likes"
            android:textColor="@color/timestamp"
            android:textColorLink="@color/link"
            android:textSize="13sp" />

        <LinearLayout
            android:id="@+id/ll_img"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/thumbImage"
            android:layout_centerHorizontal="true"

            android:layout_gravity="center_horizontal"
            android:gravity="center"
            android:orientation="horizontal"
            android:weightSum="3" >

            <TextView
                android:id="@+id/btn_like"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="@drawable/grid_row_selector"
                android:drawableLeft="@mipmap/ic_like"
                android:drawablePadding="33dp"
                android:paddingLeft="13dp"
                android:text="Like1"
                android:textColor="#66000000"
                android:textSize="12sp" />

            <View
                android:layout_width=".5dp"
                android:layout_height="match_parent"
                android:background="#11000000" />
            <TextView
                android:id="@+id/btn_comment"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="@drawable/grid_row_selector"
                android:drawableLeft="@mipmap/ic_comment"
                android:drawablePadding="10dp"
                android:paddingLeft="10dp"
                android:text="Comment "
                android:textColor="#66000000"
                android:textSize="12sp" />

            <View
                android:layout_width=".5dp"
                android:layout_height="match_parent"
                android:background="#11000000" />

            <TextView
                android:id="@+id/btn_share"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="@drawable/grid_row_selector"
                android:drawableLeft="@mipmap/ic_share"
                android:drawablePadding="22dp"
                android:paddingLeft="13dp"
                android:text="Share "
                android:textColor="#66000000"
                android:textSize="12sp" />
        </LinearLayout>
    </LinearLayout>
    <!-- /////////////1 ENDS 1 ///////////////layout for SHARE/////////////////// -->


    <!-- /////////////2 STARTs 2///////////////layout for CONNECT_TO/////////////////// -->

    <LinearLayout
        android:id="@+id/lnr_connect_to"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/feed_item_margin"
        android:layout_marginRight="@dimen/feed_item_margin"
        android:layout_marginTop="@dimen/feed_item_margin"
        android:background="@drawable/bg_parent_rounded_corner"
        android:orientation="vertical"
        android:paddingBottom="@dimen/feed_item_padding_top_bottom"
        android:paddingTop="@dimen/feed_item_padding_top_bottom"
        android:visibility="gone" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingLeft="@dimen/feed_item_padding_left_right"
            android:paddingRight="@dimen/feed_item_padding_left_right" >

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="2dp" >

                <ImageView
                    android:id="@+id/profilePic_connectTo"
                    android:layout_width="@dimen/feed_item_profile_pic"
                    android:layout_height="@dimen/feed_item_profile_pic"
                    android:scaleType="fitCenter" />

                <TextView
                    android:id="@+id/tv_profilePic_connectTo"
                    android:layout_width="@dimen/feed_item_profile_pic"
                    android:layout_height="@dimen/feed_item_profile_pic"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/circle_prop_bg"
                    android:gravity="center"
                    android:textColor="#FFFFFFFF"
                    android:textSize="45sp"
                    android:visibility="gone" />
            </RelativeLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingLeft="@dimen/feed_item_profile_info_padd" >

                <TextView
                    android:id="@+id/tv_connectTo_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="--"
                    android:textSize="@dimen/feed_item_profile_name"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv_connectTo_content"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="@dimen/feed_item_profile_name"
                    android:textStyle="normal" />

                <TextView
                    android:id="@+id/tv_connectFrom_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="@dimen/feed_item_profile_name" />

                <TextView
                    android:id="@+id/timestamp_connectTo"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/timestamp"
                    android:textSize="@dimen/feed_item_timestamp" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <!-- /////////////2 ENDs 2 ///////////////layout for CONNECT_TO /////////////////// -->


    <!-- /////////////3 STARTs 3///////////////layout for CONNECT_FROM/////////////////// -->
    <!-- /////////////3 ENDs 3 ///////////////layout for CONNECT_FROM /////////////////// -->


    <!-- /////////////4 STARTs 4 ///////////////layout for PROPERTY/////////////////// -->

    <LinearLayout
        android:id="@+id/lnr_property"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/feed_item_margin"
        android:layout_marginRight="@dimen/feed_item_margin"
        android:layout_marginTop="@dimen/feed_item_margin"
        android:background="@drawable/bg_parent_rounded_corner"
        android:orientation="vertical"
        android:paddingBottom="@dimen/feed_item_padding_top_bottom"
        android:paddingTop="@dimen/feed_item_padding_top_bottom"
        android:visibility="visible" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingLeft="@dimen/feed_item_padding_left_right"
            android:paddingRight="@dimen/feed_item_padding_left_right" >

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <ImageView
                    android:id="@+id/profilePic_property"
                    android:layout_width="@dimen/feed_item_profile_pic"
                    android:layout_height="@dimen/feed_item_profile_pic"
                    android:scaleType="fitCenter" />

                <TextView
                    android:id="@+id/tv_profilePic_property"
                    android:layout_width="@dimen/feed_item_profile_pic"
                    android:layout_height="@dimen/feed_item_profile_pic"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/circle_prop_bg"
                    android:gravity="center"
                    android:textColor="#FFFFFFFF"
                    android:textSize="45sp"
                    android:visibility="gone" />
            </RelativeLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingLeft="@dimen/feed_item_profile_info_padd" >

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/tv_nameProperty"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="Ravii Gupta"
                        android:textSize="@dimen/feed_item_profile_name"
                        android:textStyle="bold" />

                    <ImageView
                        android:id="@+id/menu_deleteProperty"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:padding="2dp"
                        android:src="@mipmap/three_dots_black" />
                </RelativeLayout>

                <TextView
                    android:id="@+id/tv_contentProperty"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Anil Gupta"
                    android:textSize="@dimen/feed_item_timestamp" />

                <TextView
                    android:id="@+id/timestamp_property"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="07:44 PM"
                    android:textColor="@color/timestamp"
                    android:textSize="@dimen/feed_item_timestamp" />

                <TextView
                    android:id="@+id/tv_ShareContent_Property"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="has shared his status" />
            </LinearLayout>
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <!-- 2 nd part image down -->

            <RelativeLayout
                android:id="@+id/img_2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" >


                <LinearLayout
                    android:id="@+id/lnr_tvz"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    android:layout_toRightOf="@+id/rl_img"
                    android:orientation="vertical" >

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:visibility="gone" >

                        <TextView
                            android:id="@+id/tv_postedBy"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:ellipsize="end"
                            android:maxLength="20"
                            android:text="Mahesh Goyal"
                            android:textColor="#FF000000"
                            android:textSize="13sp"
                            android:textStyle="normal" />

                        <TextView
                            android:id="@+id/tv_userType"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_toRightOf="@+id/tv_postedBy"
                            android:layout_weight="1"
                            android:background="#E2E2E2"
                            android:text="(Broker)"
                            android:textColor="#99000000"
                            android:textSize="12sp"
                            android:textStyle="italic" />

                        <RelativeLayout
                            android:id="@+id/rl_favorite"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" >

                            <ImageView
                                android:id="@+id/iv_favorite"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_alignParentRight="true"
                                android:src="@mipmap/ic_remove" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ellipsize="end"
                            android:maxLines="2"
                            android:text="Availability of 1 apartment in Rohini"
                            android:textColor="#AA000000"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height=".2dp"
                        android:layout_margin="1dp"
                        android:background="#22000000" />

                    <!-- //////////////////////Total main content //////////////// -->

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:weightSum="1" >

                        <!-- //////////////////////Start 1st part ////////////// -->

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical" >

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:orientation="horizontal" >

                                <TextView
                                    android:id="@+id/tv_rate"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:maxLength="20"
                                    android:singleLine="true"
                                    android:text="Rs.2000000"
                                    android:textColor="#AA000000"
                                    android:textSize="12sp"
                                    android:textStyle="bold" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="2dp"
                                android:gravity="center_vertical"
                                android:orientation="horizontal" >

                                <ImageView
                                    android:layout_width="10dp"
                                    android:layout_height="10dp"
                                    android:src="@mipmap/ic_property_type" />

                                <TextView
                                    android:id="@+id/tv_propertyType"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="1dp"
                                    android:ellipsize="end"
                                    android:maxLength="22"
                                    android:singleLine="true"
                                    android:text="Independent House/Villa"
                                    android:textColor="#AA000000"
                                    android:textSize="12sp" />
                            </LinearLayout>
                        </LinearLayout>
                        <!-- //////////////////////Ends 1st part ////////////// -->
                        <!-- //////////////////////Start 2st part ////////////// -->

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="6dp"
                            android:layout_marginTop="2dp"
                            android:orientation="vertical" >

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:orientation="horizontal" >

                                <ImageView
                                    android:layout_width="10dp"
                                    android:layout_height="10dp"
                                    android:src="@mipmap/ic_locations" />

                                <TextView
                                    android:id="@+id/tv_location"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="1dp"
                                    android:maxLength="20"
                                    android:singleLine="true"
                                    android:text="Gurgaon"
                                    android:textColor="#AA000000"
                                    android:textSize="12sp" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="2dp"
                                android:gravity="center_vertical"
                                android:orientation="horizontal" >

                                <ImageView
                                    android:layout_width="10dp"
                                    android:layout_height="10dp"
                                    android:src="@mipmap/ic_area" />

                                <TextView
                                    android:id="@+id/tv_area"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="1dp"
                                    android:ellipsize="end"
                                    android:gravity="center_vertical"
                                    android:maxLength="20"
                                    android:singleLine="true"
                                    android:text="1200 sq feet"
                                    android:textColor="#AA000000"
                                    android:textSize="12sp" />
                            </LinearLayout>
                        </LinearLayout>
                        <!-- //////////////////////Ends 2st part////////////// -->

                    </LinearLayout>
                    <!-- //////////////////////Ends Total Layout ////////////// -->

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal" >

                        <ImageView
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:src="@mipmap/ic_landmark" />

                        <TextView
                            android:id="@+id/tv_landmark"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="1dp"
                            android:singleLine="true"
                            android:text="Palam ViharGurgaon, Haryana, India"
                            android:textColor="#AA000000"
                            android:textSize="12sp" />
                    </LinearLayout>
                    <!-- //////////////// seperator fro layout ////////////////////////////////// -->

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:gravity="right|center_vertical"
                        android:orientation="horizontal" >

                        <ImageView
                            android:id="@+id/iv_postedTime"
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:layout_centerVertical="true"
                            android:src="@android:drawable/ic_lock_idle_alarm" />

                        <TextView
                            android:id="@+id/tv_postedTime"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@+id/iv_postedTime"
                            android:text="7.30 PM"
                            android:textColor="#77000000"
                            android:textSize="12sp" />
                    </RelativeLayout>
                    <!-- //////////////// seperator fro layout ////////////////////////////////// -->

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/thumbImage"
                        android:layout_centerHorizontal="true"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:orientation="vertical" >

                        <View
                            android:layout_width="match_parent"
                            android:layout_height=".5dp"
                            android:background="#11000000" />

                        <TextView
                            android:id="@+id/tv_TotalLikesProperty"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:paddingBottom="2dp"
                            android:paddingLeft="@dimen/feed_item_status_pad_left_right"
                            android:paddingRight="@dimen/feed_item_status_pad_left_right"
                            android:text="status message"
                            android:textColor="@color/timestamp"
                            android:textColorLink="@color/link"
                            android:textSize="13sp" />

                        <LinearLayout
                            android:id="@+id/lnr_likeProperty"
                            android:layout_width="match_parent"
                            android:layout_height="35dp"
                            android:layout_gravity="center_horizontal"
            				android:gravity="center"
                            android:layout_centerHorizontal="true"
                            android:weightSum="3" >

                            <Button
                                android:id="@+id/btn_likeProperty"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_margin="1dp"
                                android:layout_weight="1"
                                android:background="@drawable/grid_row_selector"
                                android:drawableLeft="@mipmap/ic_like"
                                android:text="Like"
